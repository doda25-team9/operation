{
  "uid": "sms-ab-dashboard",
  "title": "SMS Checker - A/B Testing Dashboard",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Total Predictions (All Results)",
      "id": 1,
      "targets": [
        {
          "expr": "rate(predictions_result_total[5m])",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "barchart",
      "title": "Model Comparison (Placeholder)",
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(predictions_result_total[5m])) by (result)",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "A/B Experiment Metadata (Placeholder)",
      "id": 3,
      "targets": [
        {
          "expr": "rate(predictions_result_total[5m])",
          "refId": "A"
        }
      ]
    }
  ]
}
